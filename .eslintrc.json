{
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": "eslint:recommended",
    "globals": {
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "indent": "off",
        "no-tabs": "error",
        "no-extra-semi": "error",
        "no-unused-vars": "warn",
        "no-var": "error"
    }
}